<!DOCTYPE html>
<html>
  <head>
    <title>HelloStocks</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type='text/javascript'>
      window.hellostockslogo = '<%= image_url('landing_page/hellostockslogo.png') %>'
